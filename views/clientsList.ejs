<%- include('header'); -%>
    <div class="content">
        <h1>Selected Clients</h1>
        <table id="clientsList">
            <thead>
                <tr>
                    <th>Num_client</th>
                    <th scope="col">Name</th>
                    <th scope="col">Society</th>
                </tr>
            </thead>
            <tbody>
                <% clientlist.forEach(clientlists => { %>
                    <tr>
                        <th scope="row">
                            <%= clientlists.num_client %>
                        </th>
                        <td>
                            <a href="/api/client/<%= clientlists.contact %>">
                                <%= clientlists.contact %>
                            </a>
                        </td>
                        <td>
                            <%= clientlists.society %>
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </div>
    <br>

<%- include('footer'); -%>